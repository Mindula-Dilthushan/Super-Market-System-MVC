<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.supermarketsystem.controller.adminview.adminotherpage.tables.ItemTableViewController">
   <children>
      <AnchorPane prefHeight="477.0" prefWidth="1091.0">
         <children>
            <TableView fx:id="customerMainTB" layoutX="-1.0" prefHeight="417.0" prefWidth="1091.0">
               <columns>
                  <TableColumn fx:id="itemCodeColumn" prefWidth="127.0" text="Item Code " />
                  <TableColumn fx:id="itemDescrColumn" prefWidth="346.0" text="Description" />
                  <TableColumn fx:id="packSizeColumn" prefWidth="212.0" text="Pack Size " />
                  <TableColumn fx:id="unitPriceColumn" prefWidth="213.0" text="Unit Price (Rs.)" />
                  <TableColumn fx:id="qtyOnHandColumn" prefWidth="191.0" text="Qty On Hand" />
               </columns>
            </TableView>
            <JFXButton fx:id="refreshBt" buttonType="RAISED" layoutX="990.0" layoutY="426.0" onAction="#refreshBtOnAction" text="Refresh ">
               <ripplerFill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop color="#ffef00" />
                        <Stop color="WHITE" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </ripplerFill>
               <font>
                  <Font name="Times New Roman Bold" size="20.0" />
               </font>
            </JFXButton>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
