        </columns>
      </TableView>
      <JFXButton fx:id="refreshBt" buttonType="RAISED" layoutX="990.0" layoutY="426.0" onAction="#refreshBtOnAction" text="Refresh ">
         <ripplerFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#ffef00" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </ripplerFill>
         <font>
            <Font name="Times New Roman Bold" size="20.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
