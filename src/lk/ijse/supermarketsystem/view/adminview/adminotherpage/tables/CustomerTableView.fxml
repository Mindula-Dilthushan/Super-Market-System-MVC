<AnchorPane prefHeight="477.0" prefWidth="1091.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.supermarketsystem.controller.adminview.adminotherpage.tables.CustomerTableViewController">
   <children>
      <TableView fx:id="customerMainTB" layoutX="-1.0" prefHeight="417.0" prefWidth="1091.0">
        <columns>
          <TableColumn fx:id="custIdColumn" prefWidth="127.0" text="Customer ID " />
            <TableColumn fx:id="custTitleColumn" prefWidth="90.0" text="Title" />
            <TableColumn fx:id="custNameColumn" prefWidth="189.0" text="Customer Name" />
            <TableColumn fx:id="custAddColumn" prefWidth="201.0" text="Customer Address" />
            <TableColumn fx:id="ciryColumn" prefWidth="167.0" text="City" />
            <TableColumn fx:id="provinceColumn" prefWidth="183.0" text="Province" />
